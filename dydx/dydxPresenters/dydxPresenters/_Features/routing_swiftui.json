                },
                "/profile/trading-rewards":{
                    "destination":"dydxPresenters.dydxTradingRewardsViewBuilder",
                    "presentation":"push"
                },
                "/portfolio":{
                    "destination":"dydxPresenters.dydxPortfolioViewBuilder"
                },
                "/portfolio/history":{
                    "destination":"dydxPresenters.dydxHistoryViewBuilder",
                    "presentation":"push"
                },
                "/portfolio/rewards":{
                    "destination":"Rewards.storyboard"
                },
                "/qrcode/display": {
                    "destination":"dydxPresenters.QRCodeDisplayBuilder",
                    "presentation":"prompt"
                },
                "/r/:code":{
                    "destination":"AffiliateReferral.xib"
                },
                "/rate_app":{
                    "destination":"dydxPresenters.dydxRateAppViewBuilder",
                    "presentation":"half"
                },
                "/restricted":{
                    "destination":"Restricted.storyboard",
                    "presentation":"half"
                },
                "/security":{
                    "destination":"dydxPresenters.dydxSecurityViewBuilder",
                    "presentation":"prompt"
                },
                "/security_at_launch":{
                    "destination":"dydxPresenters.dydxSecurityViewBuilder",
                    "presentation":"root"
                },
