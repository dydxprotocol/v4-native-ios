                    "destination":"AffiliateIndividual.storyboard"
                },
                "/affiliate/entity": {
                    "destination":"AffiliateEntity.storyboard"
                },
                "/alerts":{
                    "destination":"dydxPresenters.dydxNewsAlertsViewBuilder",
                    "presentation":"prompt"
                },
                "/closePosition/status":{
                    "destination":"dydxPresenters.dydxTradeStatusViewBuilder",
                    "presentation":"prompt"
                },
                "/compliance":{
                    "destination":"Compliance.storyboard",
                    "presentation":"half"
                },
                "/deposit":{
                    "destination":"Transfer3.storyboard",
                    "presentation":"prompt"
                },
